<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<Window size="800,500" sizebox="4,4,6,6" roundcorner="4,4" showshadow="true" shadowimage="common/shadow.png" shadowsize="6" shadowcorner="8,8,8,8">
	<Font name="微软雅黑" size="12" default="true" />
	<Font id="1" name="微软雅黑" size="14" />
	<Font id="2" name="微软雅黑" size="18" />
    <VerticalLayout bkcolor="#FFFFFFFF">
		<HorizontalLayout height="30">
			<HorizontalLayout />
			<Button name="btnClose" width="30" normalimage="DlgSetting/btnClose.png" hotimage="DlgSetting/btnCloseHot.png" />
		</HorizontalLayout>
        <HorizontalLayout>
			<VerticalLayout name="grup_set" width="140" padding="120,0,0,0">
				<Option name="optDevice" group="grup_set" selected="true" width="60" height="20" padding="0,24,0,0" textcolor="#FF999999" selectedtextcolor="#FF626a99" />
				<Option name="optCommon" group="grup_set" width="60" height="20" padding="0,20,0,0" textcolor="#FF999999" selectedtextcolor="#FF626a99" />
				<Option name="optUpdate" group="grup_set" width="60" height="20" padding="0,20,0,0" textcolor="#FF999999" selectedtextcolor="#FF626a99" />
				<Option name="optCommit" group="grup_set" width="60" height="20" padding="0,20,0,0" textcolor="#FF999999" selectedtextcolor="#FF626a99" />
			</VerticalLayout>
			<VerticalLayout>
				<TabLayout name="tabSetting" padding="0,20,0,0">
					<VerticalLayout >
						<Label name="labAudio" width="60" align="left" font="2" textcolor="#FF333333" />
						<HorizontalLayout height="20" padding="0,20,0,0">
							<Label name="labMic" width="80" align="right" textcolor="#FF666666" />
							<Combo name="comMicDevice" width="180" textpadding="5,0,0,0" itemitemtextcolor="#FF666666" bkimage="DlgSetting/comDeviceNormal.png" hotimage="DlgSetting/comDeviceHot.png" />
						</HorizontalLayout>
						<HorizontalLayout height="20" padding="0,20,0,0" >
							<Label name="labMicVolume" width="80" align="right" textcolor="#FF666666" />
							<Slider name="sliderMicVolume" height="10" width="140" padding="0,5,0,0" min="0" max="65535" bkimage="DlgSetting/sliderVolumeNormal.png" foreimage="DlgSetting/sliderVolumeHot.png" thumbimage="DlgSetting/thumbFullNormal.png" thumbhotimage="DlgSetting/thumbFullHot.png" disabledimage="DlgSetting/sliderVolumeNormal.png"/>
						</HorizontalLayout>
						<Option name="optAudioAGC" height="16" padding="50,20,0,0" textpadding="20,0,0,0" textcolor="#FF666666" align="left" normalimage="file='DlgSetting/OptNormal.png' dest='0,0,16,16'" selectedimage="file='DlgSetting/OptSelect.png' dest='0,0,16,16'"/>
						<HorizontalLayout height="20" padding="0,20,0,0" >
							<Label name="labSpeaker" width="80" align="right" font="1" textcolor="#FF666666" />
							<Combo name="comSpeakerDevice" width="180" textpadding="5,0,0,0" itemitemtextcolor="#FF666666" bkimage="DlgSetting/comDeviceNormal.png" hotimage="DlgSetting/comDeviceHot.png" />
						</HorizontalLayout>
						<HorizontalLayout height="20" padding="0,20,0,0" >
							<Label name="labSpeakerVolume" width="80" align="right" textcolor="#FF666666" />
							<Slider name="sliderSpeakerVolume" height="10" width="140" padding="0,5,0,0" min="0" max="65535" bkimage="DlgSetting/sliderVolumeNormal.png" foreimage="DlgSetting/sliderVolumeHot.png" thumbimage="DlgSetting/thumbFullNormal.png" thumbhotimage="DlgSetting/thumbFullHot.png" />
						</HorizontalLayout>
						<Label name="labVideo" width="60" align="left" font="2" padding="0,30,0,0" textcolor="#FF333333" />
						<HorizontalLayout height="20" padding="0,20,0,0">
							<Label name="labCamera" width="80" align="right" font="1" textcolor="#FF666666" />
							<Combo name="comCameraDevice" width="180" textpadding="5,0,0,0" itemitemtextcolor="#FF666666" bkimage="DlgSetting/comDeviceNormal.png" hotimage="DlgSetting/comDeviceHot.png" />
						</HorizontalLayout>
						<Option name="optCapVideo720P" height="16" padding="50,20,0,0" textpadding="20,0,0,0" textcolor="#FF666666" align="left" normalimage="file='DlgSetting/OptNormal.png' dest='0,0,16,16'" selectedimage="file='DlgSetting/OptSelect.png' dest='0,0,16,16'"/>
					</VerticalLayout>
					<VerticalLayout >
						<Label name="labCommon" width="160" align="left" font="2" textcolor="#FF333333" />
						<Label name="labApply" padding="0,30,0,0" width="60" align="left" font="1" textcolor="#FF666666" />
						<Option name="optStartWithSystem" height="16" textpadding="20,0,0,0" padding="0,12,0,0" textcolor="#FF666666" align="left" normalimage="file='DlgSetting/OptNormal.png' dest='0,0,16,16'" selectedimage="file='DlgSetting/OptSelect.png' dest='0,0,16,16'"/>
						<Option name="optHideToTray" height="16" textpadding="20,0,0,0" padding="0,12,0,0" textcolor="#FF666666" align="left" normalimage="file='DlgSetting/OptNormal.png' dest='0,0,16,16'" selectedimage="file='DlgSetting/OptSelect.png' dest='0,0,16,16'"/>
						<Option name="optCopyUrl" height="16" textpadding="20,0,0,0" padding="0,12,0,0" textcolor="#FF666666" align="left" normalimage="file='DlgSetting/OptNormal.png' dest='0,0,16,16'" selectedimage="file='DlgSetting/OptSelect.png' dest='0,0,16,16'"/>
						<Option name="optLeaveWithConfirm" height="16" textpadding="20,0,0,0" padding="0,12,0,0" textcolor="#FF666666" align="left" normalimage="file='DlgSetting/OptNormal.png' dest='0,0,16,16'" selectedimage="file='DlgSetting/OptSelect.png' dest='0,0,16,16'"/>
						<Label name="labShare" padding="0,30,0,0" width="60" align="left" font="1" textcolor="#FF666666" />
						<Option name="optDBScreen" height="16" textpadding="20,0,0,0" padding="0,12,0,0" textcolor="#FF666666" align="left" normalimage="file='DlgSetting/OptNormal.png' dest='0,0,16,16'" selectedimage="file='DlgSetting/OptSelect.png' dest='0,0,16,16'"/>
						<Option name="optFullScreen" height="16" textpadding="20,0,0,0" padding="0,12,0,0" textcolor="#FF666666" align="left" normalimage="file='DlgSetting/OptNormal.png' dest='0,0,16,16'" selectedimage="file='DlgSetting/OptSelect.png' dest='0,0,16,16'"/>
						<Option name="optShareFullScreen" height="16" textpadding="20,0,0,0" padding="0,12,0,0" textcolor="#FF666666" align="left" normalimage="file='DlgSetting/OptNormal.png' dest='0,0,16,16'" selectedimage="file='DlgSetting/OptSelect.png' dest='0,0,16,16'"/>
						<Option name="optShareAutoSize" height="16" textpadding="20,0,0,0" padding="0,12,0,0" textcolor="#FF666666" align="left" normalimage="file='DlgSetting/OptNormal.png' dest='0,0,16,16'" selectedimage="file='DlgSetting/OptSelect.png' dest='0,0,16,16'"/>
					</VerticalLayout>
					<VerticalLayout >
						<Label name="labUpdate" width="160" align="left" font="2" textcolor="#FF333333" />
						<Label name="labLocalVersion" padding="0,30,0,0" width="500" align="left" font="1" textcolor="#FF666666" />
						<Label name="labCurentVersion" padding="0,10,0,0" width="500" align="left" font="1" textcolor="#FF666666" />
						<Button name="btnUpdate" height="24" width="80" padding="0,20,0,0" font="1" textcolor="#FF626A99" hottextcolor="#FF9CA4E4" pushedtextcolor="#FF626A99" normalimage="common/btn80px.png" />
					</VerticalLayout>
					<VerticalLayout >
						<Label name="labCommit" width="160" align="left" font="2" textcolor="#FF333333" />
						<Label name="labTips" padding="0,30,0,0" width="400" align="left" font="1" textcolor="#FF666666" />
						<Label name="labTips1" padding="0,10,0,0" width="400" align="left" font="1" textcolor="#FF666666" />
						<Label name="labTips2" padding="0,10,0,0" width="400" align="left" font="1" textcolor="#FF666666" />
						<Label name="labTips3" padding="0,10,0,0" width="400" align="left" font="1" textcolor="#FF666666" />
					</VerticalLayout>
				</TabLayout>
			</VerticalLayout>
        </HorizontalLayout>
    </VerticalLayout>
</Window>
